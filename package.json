    "vue-loader": "15.7.1",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.41.0",
    "webpack-cli": "3.3.9",
    "webpack-watch-server": "1.2.1",
    "whatwg-fetch": "3.0.0"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "public/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
