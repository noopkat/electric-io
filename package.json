    "@babel/preset-env": "7.6.2",
    "@babel/preset-stage-2": "7.0.0",
    "@types/jest": "24.0.18",
    "@vue/test-utils": "1.0.0-beta.29",
    "alex": "8.1.1",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-minify-webpack-plugin": "0.3.1",
    "css-loader": "3.4.2",
    "debug": "4.1.1",
    "eslint": "6.8.0",
    "eslint-plugin-vue": "5.2.3",
    "husky": "3.1.0",
    "jest": "24.9.0",
    "jest-axe": "3.3.0",
    "nodemon": "2.0.2",
    "npm-run-all": "4.1.5",
    "prettier": "1.19.1",
